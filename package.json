{
  "name": "medium-reader-react",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "build": "tsc"
  },
  "dependencies": {
    "@types/react": "^15.0.18",
    "@types/react-native": "^0.42.9",
    "react": "15.4.2",
    "react-native": "0.42.3",
    "react-native-material-design": "^0.3.7"
  },
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "babel-jest": "^19.0.0",
    "babel-preset-react-native": "^1.9.1",
    "jest": "20.0.0",
    "react-addons-test-utils": "15.4.2",
    "react-test-renderer": "15.4.2",
    "ts-jest": "^20.0.1",
    "typescript": "^2.3.2",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "automock": false,
